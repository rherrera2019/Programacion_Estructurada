#include <locale.h>//Esta librería es para poner acentos y Ñ
int main()
{
	setlocale(LC_ALL,"");//método de la librería locale.h, para poner acentos y Ñ
	printf("EJERCICIO 13. IMPRIMIR DOS LINEAS DISTINTAS CON LAS FRASES: \n");
	printf("1) 'Bienvenido a la programación en C' y 2) 'Comenzamos a programar'\n\n");
	printf("Bienvenido a la programación en C \n");
	printf("Comenzamos a programar\n");
	printf("Teclee ENTER...");
	fflush(stdin);
	getchar();
	return 0;
}
